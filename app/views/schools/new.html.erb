<%= render "shared/header" %>
  <main>
    <%= form_with model: @school, local: true do |f| %>
      <%= render 'shared/error_messages', model: f.object %>
        <div class="school-reg">
          <%= f.text_field :name, class:"school-name", placeholder:"学校名", maxlength:"40" %>
          <%= f.submit "登録する" ,class:"reg-btn" %>
      </div>
    <% end %>
    <%= link_to 'もどる', :back, class:"back-btn" %>
  </main>